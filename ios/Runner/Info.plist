<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Food Finder</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>food_finder</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<!-- Camera and Photo Library permissions -->
	<key>NSCameraUsageDescription</key>
	<string>This app needs camera access to scan and identify food items</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This app needs photos access to let you select images of food for identification</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>This app needs microphone access for video features</string>
	
	<!-- File system access permissions -->
	<key>NSDocumentsFolderUsageDescription</key>
	<string>This app needs access to the documents folder to save food recognition data</string>
	<key>NSDownloadsFolderUsageDescription</key>
	<string>This app needs access to the downloads folder to save food recognition data</string>
	<key>NSFileProviderDomainUsageDescription</key>
	<string>This app needs access to file storage to save food recognition data</string>
	
	<!-- Network permissions -->
	<key>NSLocalNetworkUsageDescription</key>
	<string>This app needs local network access for debugging</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	
	<!-- Flutter specific settings -->
	<key>io.flutter.embedded_views_preview</key>
	<true/>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	
	<!-- Fix for iOS 14+ -->
	<key>NSBonjourServices</key>
	<array>
		<string>_dartobservatory._tcp</string>
	</array>
</dict>
</plist> 